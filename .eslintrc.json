{
    "extends": "airbnb-base",
    "env":{
        "browser":true,
        "node":true
    },
    "rules":{
        "func-names":"off",
        "no-case-declarations":"off",
        "no-undef":"off",
        "no-unreachable":"off",
        "import/no-cycle":"off",
        "no-plusplus":"off"
    }
}